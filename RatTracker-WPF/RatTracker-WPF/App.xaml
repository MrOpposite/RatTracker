<Application x:Class="RatTracker_WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RatTracker_WPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <SolidColorBrush x:Key="brushEliteRed" Color="#FFCE050F" />
        <SolidColorBrush x:Key="brushEliteBrightOrange" Color="#FFFFBD32" />
        <SolidColorBrush x:Key="brushEliteOrange" Color="#FFFF7100"/>
        <SolidColorBrush x:Key="brushEliteGreen" Color="#FF029E4C"/>
        <SolidColorBrush x:Key="brushEliteBlue" Color="#FF00B3F7"/>
        <SolidColorBrush x:Key="brushTransparent" Color="#00000000"/>

    </Application.Resources>
</Application>
